# Backend Environment Variables
# Copy this file to .env in the backend directory and fill in your actual values

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/mscafe_coffeeshop
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/mscafe_coffeeshop

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here
SESSION_SECRET=your-session-secret-key-here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Server Configuration
PORT=3000
NODE_ENV=development

# Optional: Client URL for CORS
CLIENT_URL=http://localhost:3000
